        <script type="text/javascript">
        jQuery(function () {
            // ios 地址替换
            if (IsIos()) {
                jQuery("embed").each(function (count) {
                    SetIosUrlToHTML(this);
                });
            }
        });

        // 获取ios地址[目前支持：优酷、搜狐、土豆、新浪、56] thisEmbed：普通视频承载embed控件。
        function SetIosUrlToHTML(thisEmbed) {
            var height = jQuery(thisEmbed).attr("height");
            var width = jQuery(thisEmbed).attr("width");
            var swfUrl = jQuery(thisEmbed).attr("src");
            var iosUrl = "";
            if (null != swfUrl && "" != swfUrl) {
                if (swfUrl.toLocaleLowerCase().indexOf("youku.com") > 0) { // 优酷
                    iosUrl = "http://v.youku.com/player/getRealM3U8/vid/" +
                        swfUrl.substring(swfUrl.lastIndexOf("sid/") + 4, swfUrl.lastIndexOf("/"));
                    addIosHtml(height, width, iosUrl, thisEmbed); // ios 代码添加
                }
                else if (swfUrl.toLocaleLowerCase().indexOf("vrs.sohu.com") > 0) { // 搜狐
                    jQuery.getJSON("http://kankan.uuu9.com/GetSohuIosUrl.ashx?jsoncallback=?", { "vid": swfUrl.substring(swfUrl.lastIndexOf("com/") + 4, swfUrl.lastIndexOf("/")) }, function (data) {
                        if (null != data) {
                            iosUrl = data.iosurl;
                            addIosHtml(height, width, iosUrl, thisEmbed); // ios 代码添加
                        }
                    });
                }
                else if (swfUrl.toLocaleLowerCase().indexOf("tudou.com") > 0) { // 土豆
                    var getCode = swfUrl.substring((swfUrl.lastIndexOf("/v/") + 3), swfUrl.indexOf("/&"));
                    jQuery.getJSON("http://v.tudou.com/RealtimeDecodeDispatcher/DispatchServlet?jsoncallback=?&iid=&type=8", { "code": getCode }, function (data) {
                        if (null != data) {
                            iosUrl = data.src;
                            addIosHtml(height, width, iosUrl, thisEmbed); // ios 代码添加
                        }
                    });
                }
                else if (swfUrl.toLocaleLowerCase().indexOf("sina.com") > 0) { // 新浪
                    var vid = swfUrl.substring((swfUrl.lastIndexOf("vid=") + 4), swfUrl.indexOf("_"));
                    jQuery.getJSON("http://video.sina.com.cn/interface/video_ids/video_ids.php?callback=?", { "v": vid }, function (data) {
                        if (null != data) {
                            iosUrl = "http://v.iask.com/v_play_ipad.php?vid=" + data.ipad_vid;
                            addIosHtml(height, width, iosUrl, thisEmbed); // ios 代码添加
                        }
                    });
                }
                else if (swfUrl.toLocaleLowerCase().indexOf("56.com") > 0) { // 56
                    jQuery.getJSON("http://kankan.uuu9.com/Get56IosUrl.ashx?jsoncallback=?", { "textid": swfUrl.substring(swfUrl.lastIndexOf("v_") + 2, swfUrl.lastIndexOf(".swf")) }, function (data) {
                        if (null != data) {
                            iosUrl = data.iosurl;
                            addIosHtml(height, width, iosUrl, thisEmbed); // ios 代码添加
                        }
                    });
                }
                else {
                    // 其他视频站（暂不支持ios设备）
                    jQuery(thisEmbed).parent().append("<span style='color:red;'>此视频暂不支持ios设备！<span>");
                }
            }
        }
        // ios url添加
        function addIosHtml(height, width, iosUrl, thisEmbed) {
            jQuery(thisEmbed).css("display", "none");
            jQuery(thisEmbed).parent().append('<video height="' + height + '" width="' + width + '" src="' + iosUrl + '" controls></video>');
        }

        // 是否ios系统判断
        function IsIos() {
            var sUserAgent = navigator.userAgent.toLowerCase();
            var bIsIpad = sUserAgent.match(/ipad/i) == "ipad";
            var bIsIphoneOs = sUserAgent.match(/iphone os/i) == "iphone os";
            var bIsMidp = sUserAgent.match(/midp/i) == "midp";
            var bIsAndroid = sUserAgent.match(/android/i) == "android";
            var bIsUc7 = sUserAgent.match(/rv:1.2.3.4/i) == "rv:1.2.3.4";
            var bIsCE = sUserAgent.match(/windows ce/i) == "windows ce";
            var bIsWM = sUserAgent.match(/windows mobile/i) == "windows mobile";
            var bIsUc = sUserAgent.match(/ucweb/i) == "ucweb";
            if (bIsIpad || bIsIphoneOs) {
                return true;
            }
            return false;
        }

