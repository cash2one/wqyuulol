.zuhe_maibot_hui{ height:38px;  margin-top:2px; line-height:38px; font-size:16px; background:#686868; text-align:center; color:#FFF; display:block;}
.zuhe_maibot_hui:hover{background:#686868; text-decoration:none; cursor:no-drop;}
.zuhe_mid_bj{height:226px; width:38px; background:url(//js01.daoju.qq.com/zb/lolriotmall/pc/images/show/shop_list.png) repeat-y center left; position:fixed; right:0px; top:0px;}
.zuhe_bar_ext{z-index:999; position:relative;}
.zuhe_time{position:absolute; top:10px; right:10px; z-index:999; background:url(//js01.daoju.qq.com/zb/lolriotmall/pc/images/show/zuhe_bj.png) no-repeat -47px -173px; height:27px; line-height:27px;}
.zuhe_time span.zuhe_timespan{ float:left; display:block; width:26px; padding-right:13px; color:#FFF; text-align:center; font-size:14px;}

/*-主框体-*/
.mainbar{background:#FFF; height:auto; padding-top:16px; }
  
.fix{position:fixed}

/*zuhe_zp*/
.zuhe_zp{cursor:pointer;}
.zuhe-ask{display:inline-block;margin:6px 0 0 5px;*margin-top:0;vertical-align: top;width:16px;height:16px;background:url(//js01.daoju.qq.com/zb/lolriotmall/pc/images/ico-ask.png) no-repeat;}
.zpoverlay{width:348px;height:328px;background:#fff;box-shadow:0 0 20px rgba(0,0,0,.1);position:absolute;top:100px;left:500px;z-index:999;display:none;}
.zpoverlay .zpol-btn-close{position:absolute;top:12px;right:12px;width:12px;height:12px;background-position:-115px -25px;text-indent:-9999em;}
.zpoverlay .zpol-tit{width:100%;height:36px;line-height:36px;background:#efefef;color:#333;font-size:14px;text-indent:18px;}
.zpoverlay .zpoverlay-main{width:100%;height:280px;overflow-y:auto;}
.zpoverlay .zpoverlay-list{margin-left:20px;padding-bottom:18px;width:300px;height:auto;border-bottom:1px solid #e2e2e2;}
.zpoverlay .zpol-list-tip{margin-top:15px;width:100%;height:15px;line-height:15px;font-size:14px;color:#666;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.zpoverlay .zpol-listul,.zpoverlay .zpol-listul li{width:100%;}
.zpoverlay .zpol-listul li{margin-top:12px;height:50px;}
.zpoverlay .zpol-listul li .zpol-djimg{width:50px;height:50px;}
.zpoverlay .zpol-listul li .zpol-djinfo{margin-left:12px;width:235px;overflow:hidden;}
.zpoverlay .zpol-listul li .zpol-djname{width:100%;height:18px;line-height:18px;color:#333;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;font-size:14px;}
.zpoverlay .zpol-listul li .zpol-djpri{margin-top:4px;color:#ed3029;line-height:16px;}
.mycart-item-sel{width:194px;padding:4px 8px;position:relative;border:1px solid transparent;color:#999;}
.mycart-item-sel .ico-xg{position:absolute;top:-1px;right:-1px;width:19px;height:18px;background-position:-83px -22px;display:none;}
.buylist .mycart-item-sel{cursor:pointer;}
.buylist:hover .mycart-item-sel{border:1px dashed #cacaca;}
.buylist:hover .mycart-item-sel .ico-xg{display:block;}
.buylist .mycart-item-sel:hover,.mycart-item-ed{border:1px dashed #ed3029;}
.buylist .mycart-item-sel:hover .ico-xg,.mycart-item-ed .ico-xg{background-position:-54px -22px;}
.buy_disable .mycart-item-sel,.buy_disable .mycart-item-sel:hover{border-color:transparent!important;cursor:default;}
.buy_disable .mycart-item-sel .ico-xg{display:none!important;}
.overlay{background:#fff;padding:8px 24px;border:1px solid #eee\0;*border:1px solid #eee;box-shadow:0 0 20px rgba(0,0,0,.1);position:absolute;top:0;left:0;z-index:999;display:none;}
.overlay .overlay-arrow{width:0;height:0;border:5px solid;border-color:transparent transparent #fff transparent;position:absolute;top:-10px;left:10%;}
.overlay-selist{padding:16px 0 6px;}
.overlay-selist li{width:240px;position:relative;padding-left:48px;zoom:1;padding-bottom:10px;}
.overlay-selist b{line-height:24px;font-weight:normal;color:#999;position:absolute;top:0;left:0;}
.overlay-selist span{min-width:28px;padding:0 8px;text-align:center;color:#333;line-height:22px;margin-bottom:10px;border:1px solid #ddd;margin-right:8px;display:inline-block;vertical-align:top;*display:inline;zoom:1;cursor:pointer;}
.overlay-selist span.on{border:1px solid #ed3029;color:#ed3029;}
.overlay-btnbox{padding-bottom:20px;}
.overlay-btn-sure{padding:6px 30px;color:#fff;text-decoration:none;background:#ed3029;}
.overlay-btn-sure:hover{text-decoration:none;}
.overlay-btn-cancel{color:#0d7ecd;padding-left:20px;}