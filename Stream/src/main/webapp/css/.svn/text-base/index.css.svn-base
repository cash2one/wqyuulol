.m-champion-team-container{height:150px;overflow:hidden;margin-top:15px}
.m-champion-team-container li{float:left}
.m-champion-team-container h4{color:#a58960;float:left;font-size:26px;font-weight:700;margin-top:55px}
.m-champion-team-container .m-champion-team-info{float:left;height:150px;overflow:hidden;margin-left:90px}
.m-champion-team-container .m-champion-team-info img{float:left;height:150px;width:auto}
.m-team-intro{margin:34px 0 0 17px;height:150px;float:left;color:#a58960;font-family:unitedsans}
.m-team-intro p{font-size:14px;height:50px;line-height:50px}
.m-team-intro p span{font-size:26px;vertical-align:middle;margin-right:4px}
.m-team-intro h5{font-size:22px;line-height:26px}
.m-champion-team-table{border-bottom:#253c5b 1px solid;width:100%;height:48px;min-width:1240px;margin:30px auto;overflow:visible;margin-bottom:20px}
.m-champion-team-title-container{width:1240px;height:60px;margin:0 auto;overflow:hidden}
.m-champion-team-title-container{width:1300px}
.m-champion-team-title-container li{cursor:pointer;list-style-type:none;width:150px;float:left;height:60px;overflow:hidden;color:#4c6c98;text-align:center;position:relative;margin-right:30px;box-sizing:border-box;padding-top:5px}
.m-champion-team-title-container li:after{content:'';display:block;position:absolute;width:9px;height:9px;background:#4c6c98;-moz-border-radius:50%;-webkit-border-radius:50%;border-radius:50%;left:50%;margin-left:-5px;bottom:7px}
.m-champion-team-title-container li.selected{color:#a58960;font-size:18px;font-weight:700;padding-top:0}
.m-champion-team-title-container li.selected:after{background:#a58960}
.m-champion-team-title-container li.selected:before{content:'';display:block;position:absolute;width:21px;height:21px;-moz-border-radius:50%;-webkit-border-radius:50%;border-radius:50%;border:1px #a58960 solid;bottom:0;left:50%;margin-left:-12px}
.yscoin-close,.yscoin-vs{background-image:url(../images/yscoin-spr.png);background-repeat:no-repeat}
.yscoin-close{background-position:-45px 0;height:17px;width:17px}
.yscoin-vs{background-position:-62px 0;height:32px;width:44px}
.yscoin-box{position:absolute;top:-4px;right:0}
.yscoin-btn-tclose{display:block;width:38px;height:20px;line-height:20px;font-size:12px;text-align:center;color:#5879a5;background:#122b4d}
.yscoin-btn-tclose:hover{text-decoration:none;background:#a58960;color:#fff}
.yscoin-countdown{font-size:12px;color:#a58960;height:32px;background:url(../images/yscoin-countdown.gif) no-repeat center right}
.yscoin-countdown .yscoin-box-icon{position:absolute;top:-26px;left:-38px;background:url(../images/yscoin-countdown-box.png) no-repeat 0 0;width:70px;height:70px}
.yscoin-countdown p{height:32px;line-height:32px;text-indent:28px;padding-right:7px}
.yscoin-countdown .yscoin-btn-rule{display:block;position:absolute;top:-6px;left:-22px;width:130px;height:42px}
.yscoin-countdown .yscoin-close{display:block;position:absolute;top:-8px;right:-8px}
.yscoin-get a{color:#a58960;font-size:12px;display:block;width:82px;height:32px;line-height:32px;text-indent:26px;background:url(../images/yscoin-countdown.gif) no-repeat center right}
.yscoin-get .yscoin-get-box{position:absolute;top:-24px;left:-42px;background:url(../images/yscoin-box.png) no-repeat 0 0;width:70px;height:70px}
.yscoin-pop-rule{outline:0;width:270px;height:186px;position:absolute;left:50%;top:50%;margin:-93px 0 0 -135px;background:url(../images/yscoin-bg.jpg) repeat;z-index:2}
.yscoin-pop-rule h5{font-size:16px;font-weight:400;color:#a58960;height:20px;line-height:20px;margin-bottom:18px}
.yscoin-pop-rule p{border-top:1px solid #0c1d33;border-bottom:1px solid #0c1d33;font-size:12px;color:#5f7da5;line-height:2;padding:4px 0}
.yscoin-pop-rule .yscoin-btn-dnd{font-size:12px;color:#1d4270;text-decoration:underline;float:right;margin:8px 0 0}
.yscoin-pop-rule .yscoin-btn-dnd:hover{color:#a58960;text-decoration:underline}
.yscoin-pop-rule .yscoin-btn-tclose{position:absolute;top:20px;right:20px}
.yscoin-count-close{color:#5f7da5}
.yscoin-pop-quiz{outline:0;width:270px;height:214px;position:absolute;left:50%;top:50%;margin:-107px 0 0 -135px;background:url(../images/yscoin-bg.jpg) repeat;overflow:hidden;text-align:center}
.yscoin-pop-quiz h5{font-size:14px;font-weight:400;color:#5f7da5;text-align:left;padding:0 0 14px;margin:0 0 18px;border-bottom:1px solid #0c1d33}
.yscoin-pop-quiz h5 span{font-family:unitedsans;font-size:20px;color:#a58960}
.yscoin-pop-quiz .yscoin-btn-tclose{margin:10px auto 0}
.yscoin-pop-con{padding:20px;position:relative}
.yscoin-step h2{font-size:14px;font-weight:400;color:#fff;text-align:left;margin:8px 0}
.yscoin-step .btn-guess{display:block;width:230px;height:50px;line-height:50px;color:#f5e9d4;font-size:16px;background:#a58960;margin:0 0 12px}
.yscoin-pop-txt{outline:0;width:270px;height:114px;position:absolute;left:50%;top:50%;margin:-57px 0 0 -135px;background:url(../images/yscoin-bg.jpg) repeat;overflow:hidden;text-align:center;display:table}
.yscoin-pop-txt .yscoin-pop-con{display:table-cell;vertical-align:middle;padding:0 20px}
.yscoin-pop-txt .yscoin-pop-con p{font-size:14px;color:#5f7da5}
.yscoin-pop-txt .yscoin-pop-con .yscoin-count-close{margin:8px 0 0;color:#a58960}
.yscoin-pop-txt .yscoin-pop-con .yscoin-btn-tclose{margin:8px auto 0}