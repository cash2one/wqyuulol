log4j.rootLogger=INFO

log4j.logger.org.apache.velocity.runtime.log.Log4JLogChute=INFO
log4j.logger.java.sql=DEBUG
log4j.logger.org.apache.commons=INFO

### \u8f93\u51fa\u5230\u63a7\u5236\u53f0 ###
log4j.appender.stdout = org.apache.log4j.ConsoleAppender
log4j.appender.stdout.Target = System.out
log4j.appender.stdout.layout = org.apache.log4j.PatternLayout
log4j.appender.stdout.layout.ConversionPattern =%-d{HH\:mm\:ss} [ %p ]  %m%n

log4j.appender.logs = org.apache.log4j.RollingFileAppender
log4j.appender.logs.File = logs\\MMS.log
log4j.appender.logs.MaxFileSize = 3MB
log4j.appender.logs.MaxBackupIndex = 10
log4j.appender.logs.layout = org.apache.log4j.PatternLayout
log4j.appender.logs.layout.ConversionPattern =%-d{HH\:mm\:ss} [ %p ]  %m%n