package com.Stream.entity;

import java.util.Date;

public class Scomment implements java.io.Serializable {

	// Fields

	private Long id;
	private Shopqq shopqq;
	private Shouban shouban;
	private String con;
	private Date t;
	private Long score;

	// Constructors

	/** default constructor */
	public Scomment() {
	}

	/** full constructor */
	public Scomment(Shopqq shopqq, Shouban shouban, String con, Date t,
			Long score) {
		this.shopqq = shopqq;
		this.shouban = shouban;
		this.con = con;
		this.t = t;
		this.score = score;
	}

	public Long getId() {
		return this.id;
	}

	public void setId(Long id) {
		this.id = id;
	}
	public Shopqq getShopqq() {
		return this.shopqq;
	}

	public void setShopqq(Shopqq shopqq) {
		this.shopqq = shopqq;
	}
	public Shouban getShouban() {
		return this.shouban;
	}

	public void setShouban(Shouban shouban) {
		this.shouban = shouban;
	}

	public String getCon() {
		return this.con;
	}

	public void setCon(String con) {
		this.con = con;
	}

	public Date getT() {
		return this.t;
	}

	public void setT(Date t) {
		this.t = t;
	}

	public Long getScore() {
		return this.score;
	}

	public void setScore(Long score) {
		this.score = score;
	}

}