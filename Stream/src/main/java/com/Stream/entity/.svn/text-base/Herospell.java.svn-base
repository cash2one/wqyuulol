package com.Stream.entity;


import java.util.ArrayList;
import java.util.List;

public class Herospell implements java.io.Serializable {

	// Fields

	private Long id;
	private Hero hero;
	private String uu;
	private String name;
	private String description;
	private String image;
	private String tooltip;
	private List<Effect> effects = new ArrayList<Effect>(0);
	private List<Leveltip> leveltips = new ArrayList<Leveltip>(0);

	// Constructors

	/** default constructor */
	public Herospell() {
	}

	/** full constructor */
	public Herospell(Hero hero, String uu, String name, String description,
			String image, String tooltip, List<Effect> effects,
			List<Leveltip> leveltips) {
		this.hero = hero;
		this.uu = uu;
		this.name = name;
		this.description = description;
		this.image = image;
		this.tooltip = tooltip;
		this.effects = effects;
		this.leveltips = leveltips;
	}

	public Long getId() {
		return this.id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Hero getHero() {
		return this.hero;
	}

	public void setHero(Hero hero) {
		this.hero = hero;
	}

	public String getUu() {
		return this.uu;
	}

	public void setUu(String uu) {
		this.uu = uu;
	}
	public String getName() {
		return this.name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescription() {
		return this.description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getImage() {
		return this.image;
	}

	public void setImage(String image) {
		this.image = image;
	}

	public String getTooltip() {
		return this.tooltip;
	}

	public void setTooltip(String tooltip) {
		this.tooltip = tooltip;
	}

	public List<Effect> getEffects() {
		return this.effects;
	}

	public void setEffects(List<Effect> effects) {
		this.effects = effects;
	}

	public List<Leveltip> getLeveltips() {
		return this.leveltips;
	}

	public void setLeveltips(List<Leveltip> leveltips) {
		this.leveltips = leveltips;
	}

}