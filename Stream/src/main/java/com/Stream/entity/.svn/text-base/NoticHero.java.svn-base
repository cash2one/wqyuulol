package com.Stream.entity;

import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;
import java.util.Set;


public class NoticHero implements java.io.Serializable {

	// Fields

	private Long id;
	private LolNotic lolNotic;
	private String names;
	private String title1;
	private String title2;
	private String comen;
	private String img;
	private List<NoticSkill> noticSkills = new ArrayList<NoticSkill>(0);

	// Constructors

	/** default constructor */
	public NoticHero() {
	}

	/** full constructor */
	public NoticHero(LolNotic lolNotic, String names, String title1,
			String title2, String comen, String img, List<NoticSkill> noticSkills) {
		this.lolNotic = lolNotic;
		this.names = names;
		this.title1 = title1;
		this.title2 = title2;
		this.comen = comen;
		this.img = img;
		this.noticSkills = noticSkills;
	}

	public Long getId() {
		return this.id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public LolNotic getLolNotic() {
		return this.lolNotic;
	}

	public void setLolNotic(LolNotic lolNotic) {
		this.lolNotic = lolNotic;
	}

	public String getNames() {
		return this.names;
	}

	public void setNames(String names) {
		this.names = names;
	}

	public String getTitle1() {
		return this.title1;
	}

	public void setTitle1(String title1) {
		this.title1 = title1;
	}
	public String getTitle2() {
		return this.title2;
	}

	public void setTitle2(String title2) {
		this.title2 = title2;
	}

	public String getComen() {
		return this.comen;
	}

	public void setComen(String comen) {
		this.comen = comen;
	}

	public String getImg() {
		return this.img;
	}

	public void setImg(String img) {
		this.img = img;
	}

	public List<NoticSkill> getNoticSkills() {
		return this.noticSkills;
	}

	public void setNoticSkills(List<NoticSkill> noticSkills) {
		this.noticSkills = noticSkills;
	}

}