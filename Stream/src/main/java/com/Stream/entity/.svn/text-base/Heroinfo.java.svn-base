package com.Stream.entity;


public class Heroinfo implements java.io.Serializable {

	// Fields

	private Long id;
	private Hero hero;
	private Long attack;
	private Long defense;
	private Long magic;
	private Long difficulty;

	// Constructors

	/** default constructor */
	public Heroinfo() {
	}

	/** full constructor */
	public Heroinfo(Hero hero, Long attack, Long defense, Long magic,
			Long difficulty) {
		this.hero = hero;
		this.attack = attack;
		this.defense = defense;
		this.magic = magic;
		this.difficulty = difficulty;
	}
	public Long getId() {
		return this.id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Hero getHero() {
		return this.hero;
	}

	public void setHero(Hero hero) {
		this.hero = hero;
	}

	public Long getAttack() {
		return this.attack;
	}

	public void setAttack(Long attack) {
		this.attack = attack;
	}

	public Long getDefense() {
		return this.defense;
	}

	public void setDefense(Long defense) {
		this.defense = defense;
	}

	public Long getMagic() {
		return this.magic;
	}

	public void setMagic(Long magic) {
		this.magic = magic;
	}

	public Long getDifficulty() {
		return this.difficulty;
	}

	public void setDifficulty(Long difficulty) {
		this.difficulty = difficulty;
	}

}